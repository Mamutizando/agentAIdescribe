{
  "compilerOptions": {
    "target": "es2016",                                 
    "module": "commonjs",                                
    "esModuleInterop": true,                            
    "forceConsistentCasingInFileNames": true,       
    "strict": true,                                     
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {                                           /* Specify a set of entries that re-map imports to additional lookup locations. */
      "@pages/*":["pages/*"],
      "@utils/*":["utils/*"],      
    }                         
  }
}
